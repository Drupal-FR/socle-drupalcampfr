    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "platform": {
            "php": "5.6"
        }
    },
    "scripts": {
        "drupal-l10n": "DrupalComposer\\DrupalL10n\\Plugin::download",
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "post-create-project-cmd": [
            "bash ./scripts/init.sh"
        ],
        "post-install-cmd": [
            "@drupal-scaffold"
        ],
        "post-update-cmd": [
            "@drupal-scaffold"
        ]
    },
    "extra": {
        "installer-paths": {
            "www/core": [
                "type:drupal-core"
            ],
            "www/modules/contrib/{$name}": [
                "type:drupal-module",
                "type:drupal-drush"
            ],
            "www/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "www/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "www/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "www/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "www/libraries/{$name}": [
                "type:drupal-library"
            ]
        },
        "drupal-scaffold": {
            "excludes": [
                "update.php",
                "web.config",
                "sites/development.services.yml",
                "sites/example.settings.local.php"
            ]
        },
        "drupal-l10n": {
            "destination": "translations/contrib",
            "languages": [
                "fr"
            ]
        },
        "patches": {
            "drupal/bootstrap": {
                "Updating to Drupal core 8.4.0-beta1 results in not working dropdown menu": "https://www.drupal.org/files/issues/updating_to_drupal_core-2903656-3.patch"
            },
            "drupal/commerce": {
                "Display payment method in the CheckoutPane Payment informations even if only single payement method available": "https://www.drupal.org/files/issues/2907504-2.patch"
            },
            "drupal/drush_language": {
                "Convert for Drush 9": "https://www.drupal.org/files/issues/drush_language-drush_9-2914081-8.patch"
            },
            "drupal/features": {
                "Drush 9 Incompatibility": "https://www.drupal.org/files/issues/2913651-drush9-21.patch"
            },
            "drupal/redis": {
                "Add status page and some statistics": "https://www.drupal.org/files/issues/redis-statistics-7.patch"
            },
            "drupal/webform": {
                "Drush 9 support": "https://www.drupal.org/files/issues/webform-drush_9-2916800-4.patch"
            }
        }
    }
}
