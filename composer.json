    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "platform": {
            "php": "5.6"
        }
    },
    "scripts": {
        "drupal-l10n": "DrupalComposer\\DrupalL10n\\Plugin::download",
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "post-create-project-cmd": [
            "bash ./scripts/init.sh"
        ],
        "post-install-cmd": [
            "@drupal-scaffold"
        ],
        "post-update-cmd": [
            "@drupal-scaffold"
        ]
    },
    "extra": {
        "installer-paths": {
            "www/core": [
                "type:drupal-core"
            ],
            "www/modules/contrib/{$name}": [
                "type:drupal-module",
                "type:drupal-drush"
            ],
            "www/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "www/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "www/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "www/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "www/libraries/{$name}": [
                "type:drupal-library"
            ]
        },
        "drupal-scaffold": {
            "excludes": [
                ".ht.router.php",
                "update.php",
                "web.config",
                "sites/development.services.yml",
                "sites/example.settings.local.php"
            ]
        },
        "drupal-l10n": {
            "destination": "translations/contrib",
            "languages": [
                "fr"
            ]
        },
        "patches": {
            "drupal/config_inspector": {
                "Filter entities with schema errors": "https://www.drupal.org/files/issues/filter_entities_with-2843729-7.patch"
            },
            "drupal/commerce": {
                "Display payment method in the CheckoutPane Payment informations even if only single payement method available": "https://www.drupal.org/files/issues/2907504-2.patch"
            },
            "drupal/page_manager": {
                "Update 'page_manager.variant_route_filter' service according to core changes": "https://www.drupal.org/files/issues/2918564-22.patch"
            },
            "drupal/redis": {
                "Add status page and some statistics": "https://www.drupal.org/files/issues/redis-statistics-7.patch"
            }
        }
    }
}
