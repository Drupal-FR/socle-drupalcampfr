{
  "name": "Drupal-FR/socle-drupalcampfr",
  "description": "Technical base for french drupalcamps. Template inspired from drupal-composer/drupal-project.",
  "type": "project",
  "homepage": "http://drupalcamp.fr",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "Florent Torregrosa",
      "homepage": "http://drupal.fr/user/40695",
      "role": "Developer"
    },
    {
      "name": "SÃ©bastien Corbin",
      "homepage": "http://drupal.fr/user/13044",
      "role": "Themer (Nantes 2016)"
    }
  ],
  "support": {
    "issues": "https://github.com/Drupal-FR/socle-drupalcampfr/issues",
    "source": "https://github.com/Drupal-FR/socle-drupalcampfr",
    "docs": "https://github.com/Drupal-FR/socle-drupalcampfr/blob/8.x-1.x/README.md"
  },
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  },
  "require": {
    "composer/installers": "^1.0.21",
    "cweagans/composer-patches": "~1.0",
    "drupal/core": "8.2.1",
    "drush/drush": "8.1.5",
    "drupal/address": "1.0.0-rc3",
    "drupal/admin_toolbar": "1.17.0",
    "drupal/block_visibility_groups": "1.1.0",
    "drupal/contact_storage": "1.0.0-beta8",
    "drupal/entityqueue": "1.0.0-alpha4",
    "drupal/eu_cookie_compliance": "1.0.0-beta7",
    "drupal/features": "3.0.0-rc1",
    "drupal/honeypot": "1.23.0",
    "drupal/menu_attributes": "1.*",
    "drupal/migrate_plus": "3.0.0-beta1",
    "drupal/migrate_source_csv": "2.0.0-rc2",
    "drupal/migrate_tools": "3.0.0-beta1",
    "drupal/pathauto": "1.0.0-beta1",
    "drupal/r4032login": "1.*",
    "drupal/redis": "1.0.0-alpha1",
    "drupal/views_data_export": "1.0.0-alpha2",
    "drupal/bootstrap": "3.0.0",
    "abraham/twitteroauth": "0.6.2"
  },
  "require-dev": {
    "drupal/devel": "1.*",
    "drupal/devel_a11y": "1.*",
    "drupal/config_inspector": "1.*",
    "drupal/examples": "1.*",
    "drupal/pistachio": "1.*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-install-cmd": [
      "bash ../scripts/update-scaffold.sh",
      "bash ../scripts/update-libraries.sh"
    ]
  },
  "extra": {
    "installer-paths": {
      "core": ["type:drupal-core"],
      "modules/contrib/{$name}": ["type:drupal-module"],
      "profiles/contrib/{$name}": ["type:drupal-profile"],
      "themes/contrib/{$name}": ["type:drupal-theme"],
      "libraries/{$name}": ["type:drupal-library"]
    },
    "patches": {
      "drupal/entityqueue": {
        "Empty views cache when queue is updated - 1": "https://www.drupal.org/files/issues/2686623.patch",
        "Empty views cache when queue is updated - 2": "https://www.drupal.org/files/issues/2686623-7_0.patch",
        "Can't save view with entity queue relationship - 1": "https://www.drupal.org/files/issues/2643928-27-dev.patch",
        "Can't save view with entity queue relationship - 2": "https://www.drupal.org/files/issues/2643928-29.patch"
      },
      "drupal/redis": {
        "Currently Drush Cr or Cache Clear UI does not flush Redis cache": "https://www.drupal.org/files/issues/redis-drush_cr-2765895-4.patch"
      }
    }
  }
}
